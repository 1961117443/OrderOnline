
@{
    ViewData["Title"] = "型材档案";
}
<blockquote class="layui-elem-quote quoteBox">
    <form class="layui-form">
        @Html.AntiForgeryToken()
        <div class="layui-inline">
            <div class="layui-input-inline">
                <input type="text" class="layui-input searchVal" placeholder="请输入用户名称" />
            </div>
            <a class="layui-btn search_btn" data-type="reload">搜索</a>
        </div>
        <div class="layui-inline">
            <a class="layui-btn layui-btn-normal addSectionBar_btn"><i class="layui-icon">&#xe61f;</i>添加型号</a>
        </div>
        <div class="layui-inline">
            <a class="layui-btn layui-btn-danger layui-btn-normal delAll_btn"><i class="layui-icon layui-icon-delete"></i>批量删除</a>
        </div>
    </form>
</blockquote>
<table id="sectionBarList" lay-filter="sectionBarList"></table>
<!--操作-->
<script type="text/html" id="sectionBarListBar">
    <a class="layui-btn layui-btn-xs" lay-event="edit">编辑</a>
    <a class="layui-btn layui-btn-xs layui-btn-danger" lay-event="del">删除</a>
</script>
@section Scripts{
    <script type="text/javascript" src="~/layui/layui.js"></script>
    <script type="text/javascript" src="~/js/sectionBar/sectionBarList.js?_=@DateTime.Now.ToString("yyyyMMddHHmmss")"></script>
}